<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<TableSelectDialog
		title="Característica de Qualidade"
		class="sapUiPopupWithPadding sapUiSizeCompact"
		items="{path: '/CaracteristicaQualidades', sorter: {path: 'Descricao'}}"
		search="_handleValueHelpSearch"
		confirm="_handleValueHelpClose"
		cancel="_handleValueHelpClose">
		<ColumnListItem>
			<cells>
				<ObjectIdentifier
					title="{Codigo}"
					text="{Descricao}"/>
			</cells>
		</ColumnListItem>
		<columns>
			<Column width="12em">
				<header>
					<Text text="Característica" />
				</header>
			</Column>
		</columns> 
	</TableSelectDialog>
</core:FragmentDefinition>